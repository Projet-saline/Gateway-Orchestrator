{
  "name": "docker_web_app",
  "version": "1.0.0",
  "description": "Node.js on Docker",
  "author": "First Last <first.last@example.com>",
  "main": "src/app.ts",
  "scripts": {
    "start:tsc": "ts-node-dev --poll src/app.ts",
    "start:build": "tsc -w",
    "start:run": "nodemon src/app.ts",
    "start": "concurrently npm:start:*"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "body-parser": "^1.20.2",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-validator": "^7.0.1",
    "moment": "^2.29.4",
    "mysql2": "^3.6.1",
    "nodemon": "^3.0.1",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.33.0",
    "sequelize-typescript": "^2.1.5",
    "ts-node-dev": "^2.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^20.6.2",
    "@types/sequelize": "^4.28.15",
    "@types/validator": "^13.11.1",
    "concurrently": "^8.2.1",
    "typescript": "^5.2.2"
  }
}
